
<Resource
    name="jdbc/connectorDs"
    auth="Container"
    type="javax.sql.DataSource"
    driverClassName="org.postgresql.Driver"
    url="jdbc:postgresql://<%= ENV['PG_MASTER_HOST'] %>:<%= ENV['PG_MASTER_PORT'] %>/template1"
    username="<%= ENV['PG_MASTER_USER'] %>"
    maxActive="20"
    maxIdle="10"
    maxWait="-1" />

